{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Ng Serve",
        "type": "chrome",
        "request": "launch",
        "preLaunchTask": "npm: start",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Launch Chrome",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Launch Edge",
        "type": "msedge",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Attach Chrome",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Launch Chrome (Test)",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:9876/debug.html",
        "webRoot": "${workspaceFolder}"
      }
    ]
  }